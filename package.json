{
  "name": "conventional-committer",
  "version": "0.3.0",
  "description": "Commit manager",
  "main": "dist/src/index.js",
  "scripts": {
    "test": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:local": "jest --coverage",
    "build": "tsc",
    "preversion": "npm run test:local && conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "prettier": "prettier --write '**/*.{js,ts,tsx,json,css,md}'"
  },
  "bin": "./dist/src/Index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wel-shy/committer.git"
  },
  "keywords": [
    "git",
    "tool"
  ],
  "author": "Daniel Welsh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wel-shy/committer/issues"
  },
  "homepage": "https://github.com/wel-shy/committer#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "lint-staged": {
    "*.{ts,js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/inquirer": "^6.0.0",
    "coveralls": "^3.0.3",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "lint-staged": "^13.1.0",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.0.3",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.1"
  },
  "dependencies": {
    "@types/jest": "^24.0.11",
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1"
  }
}
